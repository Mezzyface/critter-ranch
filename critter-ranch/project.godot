; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Critter Ranch"
run/main_scene="res://test_scenes/TestRunner.tscn"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[autoload]

SignalBus="*res://systems/SignalBus.gd"
DayManager="*res://systems/DayManager.gd"
EconomyManager="*res://systems/EconomyManager.gd"
ActivityProcessor="*res://systems/ActivityProcessor.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[rendering]

renderer/rendering_method="mobile"
